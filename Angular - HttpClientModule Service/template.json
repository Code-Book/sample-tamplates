{
    "name": "Angular - HttpClientModule Service",
    "author": "Sanchit Gupta",
    "email": "gupta.sanchit90@gmail.com",
    "version": "1.0.0",
    "description": {
        "short": "Angular - HttpClientModule Service",
        "long": "Creating Angular - HttpClientModule Service"
    },
    "languages": [
        "Typescript",
        "Javascript"
    ],
    "frameworks": [
        "RXJS"
    ],
    "tags": [
        "RXJS",
        "Pipeable Operators"
    ],
    "parameters": [
        {
            "type": "default",
            "name": "item",
            "subType": "array",
            "title": "Operators",
            "message": "Enter the name of Operators you want to generate",
            "required": true
        },
        {
            "type": "input",
            "name": "distFolder",
            "subType": "dir",
            "title": "Destination Folder",
            "message": "Absolute directory path, leave blank to use default directory",
            "required": false
        }
    ],
    "actions": [
        {
            "type": "add",
            "path": "{{__destinationDir distFolder}}/service.ts",
            "templateFile": "{{__templatePath}}/_service.ts"
        }
    ]
}